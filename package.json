  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "preview:pm2": "vite preview --port 8040 --strictPort",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "sync:categorias": "node scripts/sync-categorias.cjs",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "db:init": "docker-compose exec mysql mysql -u rare_toy_user -p rare_toy_companion < database/init.sql",
    "db:backup": "docker-compose exec mysql mysqldump -u rare_toy_user -p rare_toy_companion > backup.sql",
    "db:restore": "docker-compose exec -T mysql mysql -u rare_toy_user -p rare_toy_companion < backup.sql",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "mysql:test": "node scripts/test-mysql-connection.js",
    "mysql:status": "mysql -u rare_toy_user -p'RareToy2025!' -e 'SHOW PROCESSLIST;'",
    "server": "node server/server.cjs",
    "server:dev": "nodemon server/server.cjs",
    "server:prod": "NODE_ENV=production node server/server.cjs",
    "dev:full": "concurrently \"npm run server\" \"npm run dev\"",
    "dev:watch": "concurrently \"npm run server:dev\" \"npm run dev\"",
    "backup": "scripts/backup.sh",
    "backup:project": "bash scripts/backup-project.sh",
    "backup:db": "scripts/backup.sh --database",
    "backup:files": "scripts/backup.sh --files",
    "backup:uploads": "scripts/backup.sh --uploads",
    "backup:configs": "scripts/backup.sh --configs",
    "backup:cleanup": "scripts/backup.sh --cleanup",
    "install:system": "chmod +x install.sh && ./install.sh",
    "pm2:start": "pm2 start ecosystem.config.cjs",
    "pm2:stop": "pm2 stop ecosystem.config.cjs",
    "pm2:restart": "pm2 restart ecosystem.config.cjs",
    "pm2:reload": "pm2 reload ecosystem.config.cjs",
    "pm2:delete": "pm2 delete ecosystem.config.cjs",
    "pm2:logs": "pm2 logs",
    "pm2:status": "pm2 status",
    "pm2:monit": "pm2 monit",
    "security:scan": "node scripts/security-scan.js",
    "performance:audit": "node scripts/performance-audit.js",
    "optimize:images": "node scripts/optimize-images.js",
    "optimize:build": "npm run build && node scripts/optimize-build.js",
    "deploy:staging": "npm run build:prod && npm run pm2:restart",
    "deploy:production": "npm run build:prod && npm run backup && npm run pm2:restart",
    "health:check": "curl -f http://localhost:3001/api/health || exit 1",
    "logs:clean": "find logs/ -name '*.log' -mtime +7 -delete",
    "cache:clear": "rm -rf node_modules/.cache && rm -rf dist",
    "clean:all": "npm run cache:clear && npm run logs:clean",
    "update:deps": "npm update && npm audit fix",
    "check:security": "npm audit && npm run security:scan"
  },
